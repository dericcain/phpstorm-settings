<templateSet group="PHP">
  <template name="test" value="/** @test */&#10;function $START$()&#10;{   &#10;&#10;}" description="Testing boilerplate" toReformat="false" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
