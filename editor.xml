<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_TRAILING_WHITESPACES_SHOWN" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
  </component>
  <component name="LiveEditOptions" updatePolicy="AUTO" />
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="d0f11" />
      <option name="BACKGROUND" value="46ff6e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\btodo-me\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="1bc829" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="cf8d2f" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="My ToDo's">
      <pattern index="2" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>