<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="10" />
    <option name="FONT_FAMILY" value="Operator Mono" />
    <option name="LINE_SPACING" value="1.8" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="3000" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
  </component>
  <component name="LiveEditOptions" updatePolicy="AUTO" />
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="d0f11" />
      <option name="BACKGROUND" value="46ff6e" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="ff5312" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="292929" />
      <option name="BACKGROUND" value="ffde3a" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bchange\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="e05eff" />
    </pattern>
    <filter name="My Fixex">
      <pattern index="1" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>